{
    "profiles": {
        "http": {
            "commandName": "Project",
            "launchBrowser": true,
            "environmentVariables": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "dotnetRunMessages": true,
            "applicationUrl": "http://localhost:5086"
        },
        "https": {
            "commandName": "Project",
            "launchBrowser": false, // HTTPSを無効化
            "environmentVariables": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "dotnetRunMessages": true,
            "applicationUrl": "http://localhost:5086" // HTTPのみ
        },
        "IIS Express": {
            "commandName": "IISExpress",
            "launchBrowser": true,
            "environmentVariables": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            }
        },
        "WSL": {
            "commandName": "WSL2",
            "launchBrowser": false, // HTTPSを無効化
            "launchUrl": "http://localhost:5086",
            "environmentVariables": {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "ASPNETCORE_URLS": "http://localhost:5086"
            },
            "distributionName": ""
        },
        "Container (Dockerfile)": {
            "commandName": "Docker",
            "launchBrowser": false, // HTTPSを無効化
            "launchUrl": "http://{ServiceHost}:{ServicePort}",
            "environmentVariables": {
                "ASPNETCORE_HTTP_PORTS": "8080"
            },
            "publishAllPorts": true,
            "useSSL": false // HTTPSを無効化
        }
    },
    "$schema": "http://json.schemastore.org/launchsettings.json",
    "iisSettings": {
        "windowsAuthentication": false,
        "anonymousAuthentication": true,
        "iisExpress": {
            "applicationUrl": "http://localhost:22650"
        }
    }
}
